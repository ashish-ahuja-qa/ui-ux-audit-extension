{
    "name": "UI/UX Audit Plugin",
    "description": "Audit current page with ChatGPT",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting", "tabs", "downloads", "notifications", "storage"],
    "host_permissions": [
      "http://localhost:5000/*"
    ],
    "action": {
      "default_title": "Audit This Page",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  